<script>
function getParameterByName(name) {

	name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "([^#]*)"),
        results = regex.exec(location.href);
    return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}



function getUrlCode() {
	
	var theError = getParameterByName("error");
	
	if (theError.indexOf("=case-id-invalid&case_id=") != -1) {
		var theid = theError.slice(25);
		$("#error-message").html("Status for this Case Number was not found. <br>Please check the number and re-enter it. <br> <br>");
		$("#case_id").val = theid;
	} else if (theError.indexOf("=captcha-error&case_id=") != -1) {
		var theid = theError.slice(23);
		$("#error-message").html("CAPTCHA answer is incorrect. <br><br>");
		$("#case_id").val = theid;
	}	else if (theError.indexOf("=app-unavailable&case_id=") != -1) {
		var theid = theError.slice(23);
		$("#error-message").html("The application is undergoing maintenance. <br>Please check back later. <br><br>");
		$("#case_id").val = theid;
	} else if (theError.indexOf("=invalid-request") != -1) {
		var theid = theError.slice(23);
		$("#error-message").html("You have made an invalid request. <br>Please enter a valid Case Number and CAPTCHA verifcation code.<br><br>");
		$("#case_id").val = theid;
	} else {
		return;
	}
}

$(function() {
	getUrlCode();
});

</script>
